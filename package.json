    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lodash": "^4.17.15",
    "markdown-loader": "^5.1.0",
    "ng-packagr": "^5.3.0",
    "ngx-bootstrap": "^5.0.0",
    "pre-commit": "^1.2.2",
    "protractor": "~5.4.0",
    "rxjs": "~6.4.0",
    "standard-version": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.0",
    "tslint": "~5.15.0",
