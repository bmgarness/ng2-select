    "@angular/router": "~8.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/marked": "^0.3.0",
    "@types/node": "~8.9.4",
    "@types/webpack": "^4.4.34",
